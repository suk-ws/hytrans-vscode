{
    
    "name": "vscode-hytrans",
    "displayName": "VSCode HyTrans",
    "description": "The Hyper Translation (hytrans) File Format supporting for VSCode.",
    
    "publisher": "suk-ws",
    "license": "MIT",
    "version": "0.3.0",
    
    "keywords": [
        "syntax",
        "highlight",
        "hyper translation",
        "hytrans",
        "aiic"
    ],
    
    "engines": {
        "vscode": "^1.60.0"
    },
    
    "categories": [
        "Programming Languages"
    ],
    
    "contributes": {
        
        "languages": [
            {
                "id": "hytrans",
                "aliases": ["Hyper Translation", "hytrans", "aiin"],
                "extensions": [".hytrans", ".aiin"],
                "configuration": "./language-configuration.json"
                
            },
            {
                "id": "hy-lang-list",
                "aliases": ["Hyper Language List", "hy-lang-list", "hylangs"],
                "extensions": [".hylangs"],
                "configuration": "./lang-list.language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "hytrans",
                "scopeName": "source.hytrans",
                "path": "./syntaxes/hytrans.tmLanguage.json"
            },
            {
                "language": "hy-lang-list",
                "scopeName": "source.hylangs",
                "path": "./syntaxes/hy-lang-list.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "hytrans",
                "path": "./snippets/hytrans.json"
            },
            {
                "language": "hy-lang-list",
                "path": "./snippets/hy-lang-list.json"
            }
        ]
        
    }
}